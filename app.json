        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juliyasemenuha.Horynize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1a9eb81c-0f46-4c9f-8ecb-9f848e63b4a1"
      }
    }
  }
